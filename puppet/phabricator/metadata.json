{
  "name": "thekevjames-phabricator",
  "version": "1.0.0",
  "author": "thekevjames",
  "summary": "Phabricator management",
  "license": "Apache-2.0",
  "source": "https://github.com/TheKevJames/puppet-phabricator",
  "project_page": "https://github.com/TheKevJames/puppet-phabricator",
  "issues_url": "https://github.com/TheKevJames/puppet-phabricator/issues",
  "dependencies": [
    {"name": "puppetlabs/stdlib", "version_requirement": ">= 4.2.0 < 5.0.0"},
    {"name": "puppetlabs/vcsrepo", "version_requirement": ">= 1.3.2 < 2.0.0"},
    {"name": "thekevjames/git", "version_requirement": ">= 1.4.2 < 2.0.0"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    { "operatingsystem": "CentOS" },
    { "operatingsystem": "Darwin" },
    { "operatingsystem": "Debian" },
    { "operatingsystem": "Fedora" },
    { "operatingsystem": "Ubuntu" }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 4.3.2 < 5.0.0" }
  ]
}
