{
  "name": "thekevjames-devbox",
  "version": "1.4.0",
  "author": "thekevjames",
  "summary": "Devbox management, for programmers",
  "license": "Apache-2.0",
  "source": "https://github.com/TheKevJames/puppet-devbox",
  "project_page": "https://github.com/TheKevJames/puppet-devbox",
  "issues_url": "https://github.com/TheKevJames/puppet-devbox/issues",
  "dependencies": [
    {"name": "puppetlabs/stdlib", "version_requirement": ">= 4.2.0 < 5.0.0"},
    {"name": "yo61/logrotate", "version_requirement": ">= 1.0.0 < 2.0.0"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    { "operatingsystem": "Darwin" },
    { "operatingsystem": "Debian" },
    { "operatingsystem": "Fedora" },
    { "operatingsystem": "Ubuntu", "operatingsystemrelease": ["12.04", "14.04"] }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 4.3.0 < 5.0.0" }
  ]
}
