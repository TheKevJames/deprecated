{
  "name": "thekevjames-vault",
  "version": "0.1.2",
  "author": "thekevjames",
  "summary": "Hashicorp Vault management",
  "license": "Apache-2.0",
  "source": "https://github.com/thekevjames/puppet-vault.git",
  "project_page": "https://github.com/thekevjames/puppet-vault",
  "issues_url": "https://github.com/thekevjames/puppet-vault/issues",
  "dependencies": [
    {"name": "puppetlabs/stdlib", "version_requirement": ">= 4.2.0 < 5.0.0"}
  ],
