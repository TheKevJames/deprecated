  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 2015.0.0 < 2016.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ]
}
